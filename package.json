{
  "name": "tmpz",
  "version": "1.0.0",
  "description": "Copy a file or directory to OS temporary directory with a random name. Also you can remove it later.",
  "main": "index.js",
  "author": "Jose Luis Quintana",
  "engines": {
    "node": ">= 4.0"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joseluisq/tmpz.git"
  },
  "dependencies": {
    "fs-extra": "^2.1.2"
  },
  "devDependencies": {
    "babel-plugin-istanbul": "^4.1.1",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-stage-2": "^6.22.0",
    "babel-register": "^6.24.0",
    "coveralls": "^2.13.0",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^10.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-node": "^4.2.2",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^2.2.0",
    "nyc": "^10.2.0",
    "readline-sync": "^1.4.7",
    "tape": "^4.6.3"
  },
  "scripts": {
    "test:tape": "tape test.js",
    "test": "npm run lint && nyc -n index.js npm run test:tape",
    "lint": "eslint index.js test.js",
    "coverage": "coveralls < ./coverage/lcov.info"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ],
    "plugins": [
      "istanbul"
    ]
  },
  "nyc": {
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": false,
    "checkCoverage": true,
    "reporter": [
      "text",
      "lcov"
    ]
  },
  "keywords": [
    "copy",
    "remove",
    "random",
    "temporary",
    "temp",
    "directory",
    "file"
  ],
  "bugs": {
    "url": "https://github.com/joseluisq/tmpz/issues"
  },
  "homepage": "https://github.com/joseluisq/tmpz#readme"
